--// SNTS KEY SYSTEM CONFIG
local keyListURL = "https://raw.githubusercontent.com/Kogoronegirito/VALID-KEY/refs/heads/main/SNTS%20VALID%20KEY"
local webhookURL = "https://discord.com/api/webhooks/1372851056945532989/p_P6FEQbrS--cSUb5CSCBlhvds6PLoi2GampE9-opmzBxOW0G7NnUaTJYM4jxK4VieHf"

--// Rounding Function
local function roundify(obj, radius)
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, radius)
    corner.Parent = obj
end

--// KEY INPUT GUI
local keyGui = Instance.new("ScreenGui", game.CoreGui)
keyGui.Name = "SNTSKeyGUI"
